{
    "python.languageServer": "Pylance",
    "python.analysis.indexing": true,
    "python.analysis.autoSearchPaths": true,
    "python.analysis.extraPaths": [
        "${workspaceFolder}"
    ],
    "files.defaultLanguage": "shellscript",
    "terminal.integrated.defaultProfile.linux": "bash",
    "files.associations": {
        "*.sh": "shellscript"
    },
    "[shellscript]": {
        "files.eol": "\n"
    }
}